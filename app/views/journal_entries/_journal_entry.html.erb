<div id="<%= dom_id journal_entry %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= journal_entry.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Content:</strong>
    <%= journal_entry.content %>
  </div>
  <div>
    <strong class="block font-medium mb-1">File path:</strong>
    <%= journal_entry.file_path %>
  </div>
</div>
